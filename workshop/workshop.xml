<mujoco model="industrial_workshop">
  <compiler angle="radian" coordinate="local" inertiafromgeom="true"/>

  <option timestep="0.002" gravity="0 0 -9.81" iterations="50" solver="Newton" tolerance="1e-10"/>

  <size njmax="500" nconmax="100" nstack="600000"/>

  <visual>
    <map force="0.1" zfar="50"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="4096"/>
    <global offwidth="800" offheight="800"/>
  </visual>

  <asset>
    <!-- 纹理定义 -->
    <texture name="floor_texture" type="2d" builtin="checker" rgb1="0.2 0.2 0.2" rgb2="0.25 0.25 0.25" width="512" height="512" mark="cross" markrgb="0.8 0.8 0.8"/>
    <texture name="metal_texture" type="2d" builtin="flat" rgb1="0.4 0.4 0.45" width="512" height="512"/>
    <texture name="wall_texture" type="2d" builtin="flat" rgb1="0.85 0.85 0.8" width="512" height="512"/>
    <texture name="conveyor_texture" type="2d" builtin="checker" rgb1="0.1 0.1 0.1" rgb2="0.15 0.15 0.15" width="512" height="512"/>
    <texture name="robot_texture" type="2d" builtin="flat" rgb1="0.95 0.5 0.1" width="256" height="256"/>
    <texture name="warning_texture" type="2d" builtin="checker" rgb1="1 0.8 0" rgb2="0.1 0.1 0.1" width="128" height="128"/>
    <texture name="box_texture" type="2d" builtin="flat" rgb1="0.6 0.4 0.2" width="256" height="256"/>

    <!-- 材质定义 -->
    <material name="floor_material" texture="floor_texture" texrepeat="10 10" specular="0.3" shininess="0.5" reflectance="0.1"/>
    <material name="metal_material" texture="metal_texture" specular="0.8" shininess="0.7" reflectance="0.3"/>
    <material name="wall_material" texture="wall_texture" specular="0.1" shininess="0.1" reflectance="0"/>
    <material name="conveyor_material" texture="conveyor_texture" texrepeat="20 2" specular="0.2" shininess="0.3"/>
    <material name="robot_material" texture="robot_texture" specular="0.6" shininess="0.5" reflectance="0.2"/>
    <material name="warning_material" texture="warning_texture" texrepeat="2 2" specular="0.1" shininess="0.1"/>
    <material name="box_material" texture="box_texture" specular="0.2" shininess="0.2"/>
  </asset>

  <default>
    <joint limited="true" damping="0.5" armature="0.1"/>
    <geom contype="1" conaffinity="1" friction="1 0.5 0.5" density="1000" margin="0.001"/>
    <motor ctrllimited="true" ctrlrange="-1 1"/>
    <position ctrllimited="true"/>
  </default>

  <worldbody>
    <!-- 照明设置 -->
    <light directional="true" pos="0 0 10" dir="0 0 -1" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3"/>
    <light directional="true" pos="5 5 8" dir="-1 -1 -2" diffuse="0.6 0.6 0.6" specular="0.2 0.2 0.2"/>
    <light directional="true" pos="-5 5 8" dir="1 -1 -2" diffuse="0.6 0.6 0.6" specular="0.2 0.2 0.2"/>

    <!-- 地面 -->
    <geom name="floor" type="plane" size="10 10 0.1" pos="0 0 0" material="floor_material" condim="3"/>

    <!-- 车间墙壁 -->
    <geom name="wall_back" type="box" size="10 0.2 4" pos="0 -8 4" material="wall_material"/>
    <geom name="wall_left" type="box" size="0.2 8 4" pos="-10 0 4" material="wall_material"/>
    <geom name="wall_right" type="box" size="0.2 8 4" pos="10 0 4" material="wall_material"/>

    <!-- 天花板钢梁 -->
    <geom name="ceiling_beam_1" type="box" size="10 0.15 0.15" pos="0 -3 7.5" rgba="0.3 0.3 0.35 1"/>
    <geom name="ceiling_beam_2" type="box" size="10 0.15 0.15" pos="0 0 7.5" rgba="0.3 0.3 0.35 1"/>
    <geom name="ceiling_beam_3" type="box" size="10 0.15 0.15" pos="0 3 7.5" rgba="0.3 0.3 0.35 1"/>

    <!-- ========== 主流水线系统 ========== -->
    <body name="conveyor_base" pos="0 0 0.85">
      <!-- 流水线支架 -->
      <geom name="conveyor_leg_1" type="cylinder" size="0.05 0.85" pos="-3.5 -0.8 -0.425" rgba="0.3 0.3 0.3 1"/>
      <geom name="conveyor_leg_2" type="cylinder" size="0.05 0.85" pos="-3.5 0.8 -0.425" rgba="0.3 0.3 0.3 1"/>
      <geom name="conveyor_leg_3" type="cylinder" size="0.05 0.85" pos="3.5 -0.8 -0.425" rgba="0.3 0.3 0.3 1"/>
      <geom name="conveyor_leg_4" type="cylinder" size="0.05 0.85" pos="3.5 0.8 -0.425" rgba="0.3 0.3 0.3 1"/>

      <!-- 流水线传送带 -->
      <geom name="conveyor_belt" type="box" size="4 0.6 0.05" pos="0 0 0" material="conveyor_material"/>

      <!-- 流水线侧边护栏 -->
      <geom name="conveyor_rail_1" type="box" size="4 0.02 0.1" pos="0 0.62 0.1" rgba="0.9 0.8 0 1"/>
      <geom name="conveyor_rail_2" type="box" size="4 0.02 0.1" pos="0 -0.62 0.1" rgba="0.9 0.8 0 1"/>

      <!-- 流水线滚轮 -->
      <geom name="conveyor_roller_1" type="cylinder" size="0.08 0.6" pos="-3 0 0.05" euler="1.5708 0 0" rgba="0.2 0.2 0.2 1"/>
      <geom name="conveyor_roller_2" type="cylinder" size="0.08 0.6" pos="-1.5 0 0.05" euler="1.5708 0 0" rgba="0.2 0.2 0.2 1"/>
      <geom name="conveyor_roller_3" type="cylinder" size="0.08 0.6" pos="0 0 0.05" euler="1.5708 0 0" rgba="0.2 0.2 0.2 1"/>
      <geom name="conveyor_roller_4" type="cylinder" size="0.08 0.6" pos="1.5 0 0.05" euler="1.5708 0 0" rgba="0.2 0.2 0.2 1"/>
      <geom name="conveyor_roller_5" type="cylinder" size="0.08 0.6" pos="3 0 0.05" euler="1.5708 0 0" rgba="0.2 0.2 0.2 1"/>
    </body>

    <!-- ========== 6轴工业机械臂 ========== -->
    <body name="robot_base" pos="-2 3 0">
      <!-- 机器人底座 -->
      <geom name="robot_platform" type="cylinder" size="0.4 0.05" rgba="0.2 0.2 0.2 1" mass="50"/>
      <geom name="robot_base_geom" type="cylinder" size="0.15 0.2" pos="0 0 0.15" material="metal_material" mass="20"/>

      <!-- 关节1: 基座旋转 -->
      <body name="link1" pos="0 0 0.35">
        <joint name="joint1" type="hinge" axis="0 0 1" range="-3.14159 3.14159" damping="2"/>
        <geom name="link1_geom" type="cylinder" size="0.12 0.15" rgba="0.95 0.5 0.1 1" mass="15"/>

        <!-- 关节2: 肩部 -->
        <body name="link2" pos="0 0 0.15" euler="0 0 0">
          <joint name="joint2" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="2"/>
          <geom name="link2_geom" type="capsule" fromto="0 0 0 0.5 0 0" size="0.08" material="robot_material" mass="12"/>

          <!-- 关节3: 肘部 -->
          <body name="link3" pos="0.5 0 0">
            <joint name="joint3" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="1.5"/>
            <geom name="link3_geom" type="capsule" fromto="0 0 0 0.45 0 0" size="0.07" rgba="0.95 0.5 0.1 1" mass="10"/>

            <!-- 关节4: 手腕旋转1 -->
            <body name="link4" pos="0.45 0 0">
              <joint name="joint4" type="hinge" axis="1 0 0" range="-3.14159 3.14159" damping="1"/>
              <geom name="link4_geom" type="capsule" fromto="0 0 0 0.15 0 0" size="0.055" material="robot_material" mass="5"/>

              <!-- 关节5: 手腕旋转2 -->
              <body name="link5" pos="0.15 0 0">
                <joint name="joint5" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="1"/>
                <geom name="link5_geom" type="cylinder" size="0.05 0.08" rgba="0.95 0.5 0.1 1" mass="3"/>

                <!-- 关节6: 手腕旋转3 -->
                <body name="link6" pos="0 0 0.08">
                  <joint name="joint6" type="hinge" axis="0 0 1" range="-3.14159 3.14159" damping="0.5"/>
                  <geom name="link6_geom" type="cylinder" size="0.045 0.03" material="robot_material" mass="2"/>

                  <!-- 末端执行器 - 夹爪 -->
                  <body name="gripper_base" pos="0 0 0.08">
                    <geom name="gripper_base_geom" type="box" size="0.06 0.04 0.03" rgba="0.3 0.3 0.3 1" mass="1"/>

                    <!-- 左夹爪 -->
                    <body name="left_finger" pos="0 0.04 0">
                      <joint name="gripper_left" type="slide" axis="0 1 0" range="0 0.04" damping="0.3"/>
                      <geom name="left_finger_geom" type="box" size="0.04 0.01 0.05" pos="0 0.02 0" rgba="0.2 0.2 0.2 1" mass="0.3"/>
                      <geom name="left_finger_tip" type="box" size="0.04 0.005 0.02" pos="0 0.035 -0.04" rgba="0.4 0.4 0.4 1"/>
                    </body>

                    <!-- 右夹爪 -->
                    <body name="right_finger" pos="0 -0.04 0">
                      <joint name="gripper_right" type="slide" axis="0 -1 0" range="0 0.04" damping="0.3"/>
                      <geom name="right_finger_geom" type="box" size="0.04 0.01 0.05" pos="0 -0.02 0" rgba="0.2 0.2 0.2 1" mass="0.3"/>
                      <geom name="right_finger_tip" type="box" size="0.04 0.005 0.02" pos="0 -0.035 -0.04" rgba="0.4 0.4 0.4 1"/>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>

    <!-- ========== 第二台机械臂 ========== -->
    <body name="robot2_base" pos="2 3 0">
      <geom name="robot2_platform" type="cylinder" size="0.4 0.05" rgba="0.2 0.2 0.2 1" mass="50"/>
      <geom name="robot2_base_geom" type="cylinder" size="0.15 0.2" pos="0 0 0.15" material="metal_material" mass="20"/>

      <body name="robot2_link1" pos="0 0 0.35">
        <joint name="robot2_joint1" type="hinge" axis="0 0 1" range="-3.14159 3.14159" damping="2"/>
        <geom name="robot2_link1_geom" type="cylinder" size="0.12 0.15" rgba="0.1 0.5 0.95 1" mass="15"/>

        <body name="robot2_link2" pos="0 0 0.15">
          <joint name="robot2_joint2" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="2"/>
          <geom name="robot2_link2_geom" type="capsule" fromto="0 0 0 0.5 0 0" size="0.08" rgba="0.1 0.5 0.95 1" mass="12"/>

          <body name="robot2_link3" pos="0.5 0 0">
            <joint name="robot2_joint3" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="1.5"/>
            <geom name="robot2_link3_geom" type="capsule" fromto="0 0 0 0.45 0 0" size="0.07" rgba="0.1 0.5 0.95 1" mass="10"/>

            <body name="robot2_link4" pos="0.45 0 0">
              <joint name="robot2_joint4" type="hinge" axis="1 0 0" range="-3.14159 3.14159" damping="1"/>
              <geom name="robot2_link4_geom" type="capsule" fromto="0 0 0 0.15 0 0" size="0.055" rgba="0.1 0.5 0.95 1" mass="5"/>

              <body name="robot2_link5" pos="0.15 0 0">
                <joint name="robot2_joint5" type="hinge" axis="0 1 0" range="-1.91986 1.91986" damping="1"/>
                <geom name="robot2_link5_geom" type="cylinder" size="0.05 0.08" rgba="0.1 0.5 0.95 1" mass="3"/>

                <body name="robot2_link6" pos="0 0 0.08">
                  <joint name="robot2_joint6" type="hinge" axis="0 0 1" range="-3.14159 3.14159" damping="0.5"/>
                  <geom name="robot2_link6_geom" type="cylinder" size="0.045 0.03" rgba="0.1 0.5 0.95 1" mass="2"/>

                  <body name="robot2_gripper_base" pos="0 0 0.08">
                    <geom name="robot2_gripper_base_geom" type="box" size="0.06 0.04 0.03" rgba="0.3 0.3 0.3 1" mass="1"/>

                    <body name="robot2_left_finger" pos="0 0.04 0">
                      <joint name="robot2_gripper_left" type="slide" axis="0 1 0" range="0 0.04" damping="0.3"/>
                      <geom name="robot2_left_finger_geom" type="box" size="0.04 0.01 0.05" pos="0 0.02 0" rgba="0.2 0.2 0.2 1" mass="0.3"/>
                      <geom name="robot2_left_finger_tip" type="box" size="0.04 0.005 0.02" pos="0 0.035 -0.04" rgba="0.4 0.4 0.4 1"/>
                    </body>

                    <body name="robot2_right_finger" pos="0 -0.04 0">
                      <joint name="robot2_gripper_right" type="slide" axis="0 -1 0" range="0 0.04" damping="0.3"/>
                      <geom name="robot2_right_finger_geom" type="box" size="0.04 0.01 0.05" pos="0 -0.02 0" rgba="0.2 0.2 0.2 1" mass="0.3"/>
                      <geom name="robot2_right_finger_tip" type="box" size="0.04 0.005 0.02" pos="0 -0.035 -0.04" rgba="0.4 0.4 0.4 1"/>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>

    <!-- ========== 工作台 ========== -->
    <body name="worktable_1" pos="-5 -3 0">
      <geom name="worktable1_leg1" type="cylinder" size="0.04 0.75" pos="-0.7 -0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable1_leg2" type="cylinder" size="0.04 0.75" pos="-0.7 0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable1_leg3" type="cylinder" size="0.04 0.75" pos="0.7 -0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable1_leg4" type="cylinder" size="0.04 0.75" pos="0.7 0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable1_top" type="box" size="0.8 0.5 0.03" pos="0 0 0.78" material="metal_material"/>
    </body>

    <body name="worktable_2" pos="5 -3 0">
      <geom name="worktable2_leg1" type="cylinder" size="0.04 0.75" pos="-0.7 -0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable2_leg2" type="cylinder" size="0.04 0.75" pos="-0.7 0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable2_leg3" type="cylinder" size="0.04 0.75" pos="0.7 -0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable2_leg4" type="cylinder" size="0.04 0.75" pos="0.7 0.4 0.375" rgba="0.3 0.3 0.3 1"/>
      <geom name="worktable2_top" type="box" size="0.8 0.5 0.03" pos="0 0 0.78" material="metal_material"/>
    </body>

    <!-- ========== 货架系统 ========== -->
    <body name="storage_rack" pos="-7 -5 0">
      <!-- 货架立柱 -->
      <geom name="rack_post1" type="box" size="0.05 0.05 2" pos="0 0 2" rgba="0.3 0.3 0.35 1"/>
      <geom name="rack_post2" type="box" size="0.05 0.05 2" pos="0 2 2" rgba="0.3 0.3 0.35 1"/>
      <geom name="rack_post3" type="box" size="0.05 0.05 2" pos="1.5 0 2" rgba="0.3 0.3 0.35 1"/>
      <geom name="rack_post4" type="box" size="0.05 0.05 2" pos="1.5 2 2" rgba="0.3 0.3 0.35 1"/>

      <!-- 货架层板 -->
      <geom name="rack_shelf1" type="box" size="0.8 1 0.02" pos="0.75 1 1" material="metal_material"/>
      <geom name="rack_shelf2" type="box" size="0.8 1 0.02" pos="0.75 1 2" material="metal_material"/>
