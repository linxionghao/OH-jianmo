      <geom name="cart_wheel1" type="cylinder" size="0.08 0.03" pos="-0.3 -0.25 0.08" rgba="0.2 0.2 0.2 1" euler="0 1.5708 0"/>
      <geom name="cart_wheel2" type="cylinder" size="0.08 0.03" pos="-0.3 0.25 0.08" rgba="0.2 0.2 0.2 1" euler="0 1.5708 0"/>
      <geom name="cart_wheel3" type="cylinder" size="0.08 0.03" pos="0.3 -0.25 0.08" rgba="0.2 0.2 0.2 1" euler="0 1.5708 0"/>
