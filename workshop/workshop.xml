      <geom name="panel_stand" type="cylinder" size="0.08 1.2" pos="0 0 0.6" rgba="0.3 0.3 0.3 1"/>
      <geom name="panel_screen" type="box" size="0.3 0.02 0.4" pos="0 0 1.3" rgba="0.1 0.1 0.15 1"/>
      <geom name="panel_display" type="box" size="0.28 0.01 0.35" pos="0 0.021 1.32" rgba="0.2 0.4 0.6 1"/>
      <geom name="panel_button1" type="cylinder" size="0.02 0.01" pos="-0.1 0.03 1" rgba="0 1 0 1" euler="1.5708 0 0"/>
      <geom name="panel_button2" type="cylinder" size="0.02 0.01" pos="0 0.03 1" rgba="1 1 0 1" euler="1.5708 0 0"/>
      <geom name="panel_button3" type="cylinder" size="0.02 0.01" pos="0.1 0.03 1" rgba="1 0 0 1" euler="1.5708 0 0"/>
