    <geom name="warning_strip_1" type="box" size="0.5 0.1 0.001" pos="-2 4.5 0.001" material="warning_material"/>
    <geom name="warning_strip_2" type="box" size="0.5 0.1 0.001" pos="2 4.5 0.001" material="warning_material"/>

    <!-- ========== 控制面板 ========== -->
