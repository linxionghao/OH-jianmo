      <geom name="cart_shelf2" type="box" size="0.4 0.3 0.02" pos="0 0 0.6" material="metal_material"/>
      <geom name="cart_shelf3" type="box" size="0.4 0.3 0.02" pos="0 0 0.9" material="metal_material"/>

