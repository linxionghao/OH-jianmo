      <geom name="rack_shelf4" type="box" size="0.8 1 0.02" pos="0.75 1 4" material="metal_material"/>
    </body>

    <!-- ========== 流水线上的物料箱 ========== -->
    <body name="box1" pos="-2 0 1.05">
      <freejoint/>
      <geom name="box1_geom" type="box" size="0.15 0.15 0.12" material="box_material" mass="2"/>
      <geom name="box1_label" type="box" size="0.151 0.08 0.001" pos="0 0 0.121" rgba="1 1 1 1"/>
    </body>

    <body name="box2" pos="0 0 1.05">
      <freejoint/>
      <geom name="box2_geom" type="box" size="0.15 0.15 0.12" material="box_material" mass="2"/>
      <geom name="box2_label" type="box" size="0.151 0.08 0.001" pos="0 0 0.121" rgba="1 1 1 1"/>
    </body>

    <body name="box3" pos="2 0 1.05">
      <freejoint/>
      <geom name="box3_geom" type="box" size="0.15 0.15 0.12" material="box_material" mass="2"/>
      <geom name="box3_label" type="box" size="0.151 0.08 0.001" pos="0 0 0.121" rgba="1 1 1 1"/>
    </body>

